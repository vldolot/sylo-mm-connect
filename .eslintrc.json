{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["next/core-web-vitals", "prettier", "eslint:recommended"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error"],
    "no-unused-vars": [
      "error",
      { "vars": "all", "args": "all", "ignoreRestSiblings": false }
    ],
    "no-undef": "error"
  }
}
